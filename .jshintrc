{
    "maxerr": 1000,
    "devel": true,

    "indent": 4,
    "maxparams": 5,
    "maxdepth": 3,
    "maxstatements": 30,
    "maxcomplexity": 10,

    "predef": {
        "chrome": false,
        "_": false
    },
    "browser": true,
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": false,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,

    "eqnull": true,
    "lastsemic": true
}
